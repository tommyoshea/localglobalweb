{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "localglobalweb.com" }
      ],
      "destination": "https://www.localglobalweb.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "protocol": "http",
      "destination": "https://www.localglobalweb.com/$1",
      "permanent": true
    }
  ]
}
